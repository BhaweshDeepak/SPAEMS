@model SMS.Contract.StudentFinance.FeeHeadMasterVM

<div class="row">
    <div class="col-md-12">
        <div class="box box-primary">
            <div class="box-header with-border">
                <h3 class="box-title">Create Fee Head Type Detail</h3>

            </div>
            @using(Ajax.BeginForm("CreateFeeHeadType","FeeHead",null,new AjaxOptions
            {
                HttpMethod = "Post",
                OnSuccess = "successMessage",
                OnComplete = "HeadTypeComplete",
                OnFailure = "failureMessage"
            },null))
            {
                <div class="box-body">
                    <div class="form-group col-md-6">
                        <label for="exampleInputEmail1">Head Type Name</label>
                        @Html.TextBoxFor(model => model.HeadName,new { @class = "form-control",placeholder = "Enter Head  name" })
                        @Html.ValidationMessageFor(model => model.HeadName)
                    </div>
                </div>
                <div class="box-footer">
                    <div class="col-md-3"></div>
                    <div class="col-md-6">
                        <input type="submit" class="btn btn-primary pull-left" value="Save" />
                        <button type="reset" class="btn btn-danger pull-Left">Cancel</button>
                    </div>
                    <div class="col-md-3"></div>
                </div>
            }
        </div>
    </div>
</div>
